#!/bin/sh

set -e

clang \
    -Wextra \
    -Wall   \
    -Wfloat-equal \
    -Wundef \
    -Wshadow \
    -Wpointer-arith \
    -Wcast-align \
    -Wstrict-prototypes \
    -Wstrict-overflow=5 \
    -Wwrite-strings \
    -Waggregate-return \
    -Wno-type-limits \
    build/b.c -o build/mb

./build/mb
